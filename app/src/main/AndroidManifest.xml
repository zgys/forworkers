<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.diting.zgy.forworkers" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/MyTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".atys.AtyMain"
            android:label="@string/title_activity_aty_main" >
        </activity>
        <activity
            android:name=".atys.AtyLogin"
            android:label="@string/title_activity_aty_login" >
        </activity>
        <activity
            android:name=".atys.AtyRest"
            android:label="@string/title_activity_aty_rest" >
        </activity>
        <activity
            android:name=".atys.AtyCheck"
            android:label="@string/title_activity_aty_check" >
        </activity>
    </application>

</manifest>
